<form  action="./admin" method="POST" autocomplete="off" class="formMensaje">
    <!-- Campo nombre del producto -->
    <div class="inputFormulario">
        <label for="title" class="textForm">Nombre</label>
        <input type="text" id="nombre" name="nombre" required>
    </div>
    <!-- Campo calidad del producto -->
    <div class="inputFormulario">
        <label for="calidad" class="textForm">Calidad</label>
        <select name="calidad" id="calidad" required>
            <option value="Alta">Alta</option>
            <option value="Media">Media</option>
            <option value="Baja">Baja</option>
            <option value="" selected disabled >Ingrese la calidad</option>
        </select>
    </div>
    <!-- Campo precio del producto -->
    <div class="inputFormulario">
        <label for="title" class="textForm">Precio</label>
        <input type="text" id="precio" name="precio" required>
    </div>
     <!-- Boton de envÃ­o -->
     <button class="subirProducto">Enviar</button>
</form>